  inherited FdQData: TFDQuery
    SQL.Strings = (
      'SELECT'
      ''
      'B.budgetid         AS PROVIDERID,'
      'b.code             as budgetcode,'
      'B.description,'
      'b.emissiondate,'
      'COUNT(A.productid) AS TOTALITENS,'
      'SUM(A.pricetotal)  AS TOTALBUDGET,'
      'lpad(c.userid, 3,  '#39'0'#39')||'#39' - '#39'|| c.name  as USERNAME,'
      'b.createdat,'
      'b.updatedat'
      ''
      'FROM'
      ''
      'tordbudgetanalyzes A'
      
        'LEFT JOIN tordbudget B ON (B.companyid = A.companyid)AND(B.code ' +
        '= A.budgetid)'
      'left join tmnguser c on (c.code = b.userid)'
      ''
      'WHERE A.companyid = :companyid'
      ''
      'GROUP BY 1,2,3,4,7,8,9')
    ParamData = <
      item
        Name = 'COMPANYID'
        DataType = ftString
        ParamType = ptInput
        Size = 64
      end>
