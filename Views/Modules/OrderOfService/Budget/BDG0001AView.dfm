  inherited FdQData: TFDQuery
    SQL.Strings = (
      'select'
      ''
      'bdg.code,'
      'bdg.budgetid,'
      'bdg.description,'
      'bdg.emissiondate,'
      'lpad(usr.userid, 3,'#39'0'#39') ||'#39'- '#39'||usr.name as username,'
      'bdg.createdat,'
      'bdg.updatedat'
      ''
      'from'
      ''
      'tordbudget bdg'
      'left join tmnguser usr on (usr.code = bdg.userid)'
      ''
      'where bdg.companyid = :companyId'
      ''
      'order by bdg.createdat desc')
