  object QueryExporta: TFDQuery
    Connection = FDataMFirebird.FDatabase
    SQL.Strings = (
      'Select'
      ''
      'c.productid as produto,'
      'c.description as descricao,'
      'b.qtde  as quantidade,'
      '0  as   preco_Unitario,'
      '0  as   Preco_Total'
      ''
      'from'
      ''
      'tordbudget a'
      
        'left join tordbudgetproducts b on (b.companyid = a.companyid)and' +
        '(b.budgetid = a.code)'
      
        'left join tstosinapiproduct c on (c.companyid = b.companyid)and(' +
        'c.code = b.productid)'
      ''
      'where a.companyid = :companyId'
      'and a.code = :code')
